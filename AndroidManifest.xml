<?xml version="1.0" encoding="utf-8"?>
<manifest package="bu.homework.shoppinglist"
      android:versionCode="1"
      android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-sdk android:minSdkVersion="8" />
    <uses-permission android:name="android.permission.SET_DEBUG_APP"></uses-permission>

    <application android:icon="@drawable/icon" android:label="@string/app_name" android:name="MyWallet">
        <activity android:name=".ShoppingListActivity"
                  android:label="@string/app_name" android:theme="@android:style/Theme.NoTitleBar" android:debuggable="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="MoneyInHandActivity"></activity>
        <activity android:name="PurchasedItemsActivity"></activity>
        <activity android:name="ItemListActivity"></activity>
        <activity android:name="ItemEditActivity"></activity>

    </application>
</manifest>